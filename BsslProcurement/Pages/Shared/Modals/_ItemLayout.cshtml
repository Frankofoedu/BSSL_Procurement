@using DcProcurement.Contexts
@model List<Stock>
@{
}
@section Styles{
}
<table id="itemTable" class="table table-striped">
    <thead>
        <tr>
            <th>Item Code</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <button class="btn btn-link btnItemName" itemName="@item.Itemdesc" value="@item.Stockno">@item.Stockno</button>
                </td>
                <td>@item.Itemdesc</td>
            </tr>
        }
    </tbody>
</table>

@section Scripts{


}